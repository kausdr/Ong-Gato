<+># Spring Application Name\r\nspring.application.name=gatosong\r\nserver.port=8081\r\n\r\n# Logging configuration using Log4j2\r\nlogging.config=classpath:log4j2.xml\r\n\r\n# Database connection URL\r\nspring.datasource.url=jdbc:mysql://localhost:3306/gatosong?allowPublicKeyRetrieval=true&rewriteBatchedStatements=true&useSSL=false&useUnicode=yes&characterEncoding=UTF-8&useLegacyDatetimeCode=true&createDatabaseIfNotExist=true&useTimezone=true&serverTimezone=UTC\r\n\r\n# Database credentials\r\nspring.datasource.username=root\r\nspring.datasource.password=PUC@123\r\n\r\n# JPA and Hibernate settings\r\nspring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL8Dialect\r\nspring.jpa.database-platform=org.hibernate.dialect.MySQL8Dialect\r\n\r\n# Hibernate ddl-auto setting (auto-create/update schema)\r\nspring.jpa.hibernate.ddl-auto=update\r\n\r\n# Hibernate Naming Strategy for table names in uppercase\r\nspring.jpa.hibernate.naming.physical-strategy=org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardImpl\r\n\r\n# SQL logging configuration\r\nspring.jpa.show-sql=true\r\nspring.jpa.properties.hibernate.format_sql=true\r\nspring.jpa.properties.hibernate.show_sql=true\r\nspring.jpa.properties.hibernate.use_sql_comments=true\r\n
